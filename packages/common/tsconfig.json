{
    "compilerOptions": {
      "target": "es5",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "module": "Commonjs",
      "outDir": "dist",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "declaration": true,
      "jsx": "react",
      "experimentalDecorators": true,
    },
    "include": [
      "src"
    ]
  }
  